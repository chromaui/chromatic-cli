{
  "name": "chromatic",
  "version": "11.3.5",
  "description": "Automate visual testing across browsers. Gather UI feedback. Versioned documentation.",
  "keywords": [
    "storybook-addon",
    "storybook",
    "addon",
    "test",
    "popular"
  ],
  "homepage": "https://www.chromatic.com",
  "bugs": {
    "url": "https://github.com/chromaui/chromatic-cli",
    "email": "support@chromatic.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chromaui/chromatic-cli.git"
  },
  "license": "MIT",
  "author": {
    "name": "Chromatic",
    "email": "support@chromatic.com"
  },
  "exports": {
    ".": {
      "types": "./isChromatic.d.ts",
      "require": "./isChromatic.js",
      "import": "./isChromatic.mjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "require": "./dist/node.js",
      "node": "./dist/node.js"
    },
    "./isChromatic": {
      "types": "./isChromatic.d.ts",
      "require": "./isChromatic.js",
      "import": "./isChromatic.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "isChromatic.js",
  "module": "isChromatic.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "./isChromatic.d.ts"
      ],
      "isChromatic": [
        "./isChromatic.d.ts"
      ],
      "node": [
        "dist/node.d.ts"
      ]
    }
  },
  "bin": {
    "chroma": "dist/bin.js",
    "chromatic": "dist/bin.js",
    "chromatic-cli": "dist/bin.js"
  },
  "files": [
    "dist/*.js",
    "dist/node.d.ts",
    "isChromatic.js",
    "isChromatic.mjs",
    "isChromatic.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build-storybook": "storybook build",
    "build-test-storybook": "cross-env SMOKE_TEST=true storybook build -o test-storybook",
    "build-subdir": "cd subdir ; pnpm run build ; cd ..",
    "chromatic": "./dist/bin.js",
    "chromatic-prebuilt": "./dist/bin.js --storybook-build-dir=\"storybook-static\"",
    "chromatic-staging": "CHROMATIC_INDEX_URL=https://www.staging-chromatic.com ./dist/bin.js",
    "chromatic-verbose": "cross-env LOG_LEVEL=verbose ./dist/bin.js",
    "lint": "pnpm run lint:js .storybook bin-src node-src test-stories ./isChromatic.js ./isChromatic.mjs",
    "lint:js": "cross-env NODE_ENV=production eslint --fix --cache --cache-location=.cache/eslint --ext .js,.json,.mjs,.ts,.cjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "release": "pnpm run build && auto shipit",
    "publish-action": "./scripts/publish-action.mjs",
    "trace": "./dist/bin.js trace",
    "trim-stats": "./dist/bin.js trim-stats-file",
    "storybook": "storybook dev -p 9009",
    "test": "vitest run --coverage && vitest run -c vitest.no-threads.config.ts",
    "test:watch": "vitest watch --coverage",
    "prepare": "husky install && npm run build",
    "dev": "tsup --watch",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm run lint:js --fix"
    ],
    "*.js": [
      "pnpm run lint:js --fix"
    ],
    "*.json": [
      "pnpm run lint:js --fix"
    ],
    "package.json": [
      "pnpm run lint:package"
    ]
  },
  "resolutions": {
    "any-observable": "^0.5.1"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@antfu/ni": "^0.21.12",
    "@auto-it/exec": "^11.1.6",
    "@chromatic-com/storybook": "^1.4.0",
    "@discoveryjs/json-ext": "^0.5.7",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/csf-tools": "^8.1.1",
    "@storybook/linter-config": "^4.0.0",
    "@storybook/react": "^8.1.1",
    "@storybook/react-webpack5": "^8.1.1",
    "@tsconfig/node16": "^16.1.3",
    "@types/archiver": "^5.3.4",
    "@types/async-retry": "^1.4.8",
    "@types/cross-spawn": "^6.0.6",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonfile": "^6.1.4",
    "@types/listr": "^0.14.9",
    "@types/node": "~18.19.33",
    "@types/picomatch": "^2.3.3",
    "@types/progress-stream": "^2.0.5",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^0.34.6",
    "ansi-html": "0.0.8",
    "any-observable": "^0.5.1",
    "archiver": "^5.3.2",
    "async-retry": "^1.3.3",
    "auto": "^11.1.6",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "cpy": "^8.1.2",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.4",
    "dotenv": "^8.6.0",
    "env-ci": "^5.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.34.1",
    "esm": "^3.2.25",
    "execa": "^7.2.0",
    "fake-tag": "^2.0.0",
    "filesize": "^10.1.2",
    "find-up": "^7.0.0",
    "formdata-node": "^6.0.3",
    "fs-extra": "^10.1.0",
    "https-proxy-agent": "^7.0.4",
    "husky": "^7.0.4",
    "jsonfile": "^6.1.0",
    "junit-report-builder": "2.1.0",
    "lint-staged": "^11.2.6",
    "listr": "0.14.3",
    "listr-update-renderer": "^0.5.0",
    "meow": "^9.0.0",
    "mock-fs": "^5.2.0",
    "no-proxy": "^1.0.3",
    "node-ask": "^1.0.1",
    "node-fetch": "3.2.10",
    "npm-run-all": "^4.1.5",
    "observable": "^2.1.4",
    "os-browserify": "^0.3.0",
    "p-limit": "3.1.0",
    "picomatch": "2.2.2",
    "pkg-up": "^3.1.0",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.8",
    "progress-stream": "^2.0.0",
    "prompts": "^2.4.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "read-pkg-up": "^7.0.1",
    "semver": "^7.6.2",
    "slash": "^3.0.0",
    "snyk-nodejs-lockfile-parser": "^1.56.1",
    "sort-package-json": "1.50.0",
    "storybook": "^8.1.1",
    "string-argv": "^0.3.2",
    "strip-ansi": "6.0.0",
    "tmp-promise": "3.0.2",
    "ts-dedent": "^1.2.0",
    "ts-loader": "^9.5.1",
    "tsup": "^7.2.0",
    "typescript": "^5.4.5",
    "util-deprecate": "^1.0.2",
    "uuid": "^8.3.2",
    "vite": "^4.5.3",
    "vitest": "^0.34.6",
    "why-is-node-running": "^2.2.2",
    "xxhash-wasm": "^1.0.2",
    "yarn-or-npm": "^3.0.1",
    "zen-observable": "^0.8.15",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@chromatic-com/cypress": "^0.*.* || ^1.0.0",
    "@chromatic-com/playwright": "^0.*.* || ^1.0.0"
  },
  "peerDependenciesMeta": {
    "@chromatic-com/cypress": {
      "optional": true
    },
    "@chromatic-com/playwright": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "auto": {
    "baseBranch": "main",
    "canary": {
      "force": true
    },
    "plugins": [
      "npm",
      "released",
      [
        "exec",
        {
          "afterShipIt": "pnpm run publish-action"
        }
      ]
    ]
  },
  "docs": "https://www.chromatic.com/docs/cli",
  "storybook": {
    "icon": "https://user-images.githubusercontent.com/263385/101995175-2e087800-3c96-11eb-9a33-9860a1c3ce62.gif",
    "displayName": "Chromatic"
  }
}